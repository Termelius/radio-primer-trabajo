            <MenuItem mnemonicParsing="false" text="4" onAction = "#PutStation"/>
            <MenuItem mnemonicParsing="false" text="5" onAction = "#AddStation"/>
            <MenuItem mnemonicParsing="false" text="5" onAction = "#PutStation"/>
